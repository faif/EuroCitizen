    </owl:inverseOf>
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:ID="visitedPlaceOf"/>
    </rdfs:subPropertyOf>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Human"/>
          <owl:Class rdf:about="#GroupOfPeople"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:range>
