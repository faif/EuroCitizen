  <owl:Class rdf:ID="BeliefSystem">
    <owl:disjointWith>
      <owl:Class rdf:ID="Culture"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="GeographicLocation"/>
