    <owl:disjointWith rdf:resource="#CombatSport"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#TeamSport"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Christianity">
    <rdfs:comment xml:lang="en">A monotheistic system of beliefs and practices based on the Old Testament and the teachings of Jesus as embodied in the New Testament and emphasizing the role of Jesus as savior.</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ReligiousBeliefSystem"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#TeamSport">
    <rdfs:comment xml:lang="en">Sports that involve teams.</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#TargetSport"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Sport"/>
    </rdfs:subClassOf>
    <owl:disjointWith rdf:resource="#CombatSport"/>
    <owl:disjointWith rdf:resource="#SnowBoardSport"/>
    <owl:disjointWith>
